{
  "name": "postcss-easywyg-describe",
  "version": "1.0.1",
  "description": "Parse special Easywyg comments placed in CSS file and generate JSON output",
  "main": "build/plugin.js",
  "scripts": {
    "test": "$(npm bin)/mocha --require babel-core/register",
    "transpile": "$(npm bin)/babel src -d build",
    "prepublish": "npm run transpile",
    "postpublish": "rm -rf build"
  },
  "keywords": [
    "parse",
    "json",
    "easywyg",
    "wysiwyg",
    "rte",
    "css",
    "comments"
  ],
  "author": "Andrew Kozlov <demerest@gmail.com>",
  "license": "MIT",
  "repository": "easywyg/postcss-easywyg-describe",
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.17",
    "babel-preset-es2015": "^6.3.13",
    "mocha": "^2.3.4"
  },
  "dependencies": {
    "postcss": "^5.0.12"
  }
}
